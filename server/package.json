{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . \"ASRC Server\" --platform=linux --arch=x64 --asar --out=build --ignore=node_modules/electron-*",
    "package-win": "electron-packager . \"ASRC Server\" --platform=win32 --arch=x64 --asar --out=build --ignore=node_modules/electron-*"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^13.1.3",
    "electron-packager": "^15.3.0"
  }
}
